generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//file schema 
model file {
  id        String   @id @default(uuid())
  fileName  String
  fileType  String
  s3Key     String   @unique
  createdAt DateTime @default(now())
}

//userSchema

model user {
  id         String @id @default(uuid())
  username   String @unique
  email      String @unique
  password   String 
}